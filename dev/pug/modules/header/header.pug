-var navList = [{name:'Видео', link: 'video.html'}, {name:'Конкурс', link: 'event.html'}, {name:'Правила', link: 'rules.html'}, {name:'Призы', link: '#'}, {name:'Планер', link: 'planer.html'}];

header.header
    .header__logo
        a(href="index.html" target="_self").header__logo-link
            img(src="static/images/content/main-logo.svg" alt="Снова в школу!").header__logo-img
    nav.header__nav.nav
        ul.nav__list
            for i in navList
                li.nav__item
                    a(href="" +i.link target="_self").nav__link=i.name
        .nav__controls
            a(href="#" target="_self").nav__controls-login Войти через VK
            button(type="button").nav__controls-toggle.js-menu-toggle
        ul.nav__social
            +socialItem('Instagram', '#', 'fa-instagram')
            +socialItem('Instagram', '#', 'fa-vk')
            +socialItem('Instagram', '#', 'fa-youtube')
